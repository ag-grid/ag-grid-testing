{
  "name": "ag-grid-testing",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "download-examples": "curl https://grid-staging.ag-grid.com/debug/all-examples.json > config/all-examples.json",
    "download-examples-local": "curl https://localhost:4610/debug/all-examples.json > config/all-examples.json",
    "download-examples-prod": "curl https://ag-grid.com/debug/all-examples.json > config/all-examples.json",
    "download-docs-pages-local": "curl https://localhost:4610/debug/all-docs-pages.json > config/all-docs-pages.json",
    "reactFunctional-examples": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/reactFunctional-results.xml playwright test reactFunctional-examples.spec.ts --reporter=junit",
    "reactFunctionalTs-examples": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/reactFunctionalTs-results.xml playwright test reactFunctionalTs-examples.spec.ts --reporter=junit",
    "angular-examples": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/angular-results.xml playwright test angular-examples.spec.ts --reporter=junit",
    "vue3-examples": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/vue3-results.xml playwright test vue3-examples.spec.ts --reporter=junit",
    "vanilla-examples": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/vanilla-results.xml playwright test vanilla-examples.spec.ts --reporter=junit",
    "typescript-examples": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/typescript-results.xml playwright test typescript-examples.spec.ts --reporter=junit",
    "ag-grid-examples": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/ag-grid-results.xml playwright test ag-grid-examples.spec.ts --reporter=junit",
    "ag-grid-demos": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/ag-grid-demos.xml playwright test ag-grid-demos.spec.ts --reporter=junit",
    "all-examples": "npm run ag-grid-demos && npm run reactFunctional-examples && npm run reactFunctionalTs-examples && npm run angular-examples && npm run vue3-examples && npm run vanilla-examples && npm run typescript-examples",
    "regression:grid:production-snapshots": "dotenvx run -f regression/.env.grid.production -- npx playwright test --config regression/playwright.config.ts --update-snapshots",
    "regression:grid:staging": "dotenvx run -f regression/.env.grid.staging -- npx playwright test --config regression/playwright.config.ts",
    "regression:grid:local": "dotenvx run -f regression/.env.grid.local -- npx playwright test --config regression/playwright.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ag-grid/ag-grid-testing.git"
  },
  "keywords": [
    "cypress",
    "ag-grid"
  ],
  "author": "Stephen Cooper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid-testing/issues"
  },
  "homepage": "https://github.com/ag-grid/ag-grid-testing/blob/main/README.md",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.6.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@types/node": "^20.11.16",
    "commander": "^9.3.0",
    "lodash": "^4.17.21"
  }
}
