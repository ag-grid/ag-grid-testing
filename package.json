{
  "name": "ag-grid-examples-cypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "download-examples": "curl https://grid-staging.ag-grid.com/debug/all-examples.json > config/all-examples.json",
    "reactFunctional-examples": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/reactFunctional-results.xml npx playwright test reactFunctional-examples.spec.ts --reporter=junit",
    "reactFunctionalTs-examples": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/reactFunctionalTs-results.xml npx playwright test reactFunctionalTs-examples.spec.ts --reporter=junit",
    "angular-examples": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/angular-results.xml npx playwright test angular-examples.spec.ts --reporter=junit",
    "vue-examples": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/vue-results.xml npx playwright test vue-examples.spec.ts --reporter=junit",
    "vue3-examples": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/vue3-results.xml npx playwright test vue3-examples.spec.ts --reporter=junit",
    "vanilla-examples": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/vanilla-results.xml npx playwright test vanilla-examples.spec.ts --reporter=junit",
    "typescript-examples": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=example-results/typescript-results.xml npx playwright test typescript-examples.spec.ts --reporter=junit",
    "all-examples": "npm run reactFunctional-examples && npm run reactFunctionalTs-examples && npm run angular-examples && npm run vue-examples && npm run vue3-examples && npm run vanilla-examples && npm run typescript-examples",
    "test": "npm run test-api-docs",
    "test-api-docs": "cypress run --spec 'cypress/e2e/ag-grid/doc-pages.cy.js' --headless  -b chrome --reporter-options 'mochaFile=test-results/api-docs.xml'",
    "test-snippet-docs": "cypress run --spec 'cypress/e2e/ag-grid/snippet-pages.cy.js' -b chrome --reporter-options 'mochaFile=test-results/snippet-docs.xml'",
    "test-snapshots": "node run-snapshots.js  --base-url $CYPRESS_BASE_URL",
    "test-config": "node run-config.js --base-url $CYPRESS_BASE_URL",
    "test-config-charts": "node run-config.js --base-url $CYPRESS_BASE_URL --charts",
    "update-snapshots": "node run-snapshots.js -- updateSnapshots"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StephenCooper/ag-grid-examples-cypress.git"
  },
  "keywords": [
    "cypress",
    "ag-grid"
  ],
  "author": "Stephen Cooper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StephenCooper/ag-grid-examples-cypress/issues"
  },
  "homepage": "https://github.com/StephenCooper/ag-grid-examples-cypress#readme",
  "dependencies": {
    "cypress-fail-on-console-error": "^2.1.4",
    "cypress-log-to-output": "^1.1.2"
  },
  "devDependencies": {
    "@cypress/skip-test": "^2.6.1",
    "@playwright/test": "^1.41.2",
    "@types/node": "^20.11.16",
    "commander": "^9.3.0",
    "cypress": "^10.8.0",
    "cypress-image-snapshot": "^4.0.1",
    "cypress-multi-reporters": "^1.6.1",
    "lodash": "^4.17.21"
  }
}
